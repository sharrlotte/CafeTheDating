{
  "hosting": {
    "site": "thedatingcoffee",
    "public": "./",
    "ignore": [
      "firebase.json",
      "database-rules.json",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.@(js|html)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=0"
          }
        ]
      }
    ]
  }
}
